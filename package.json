{
  "name": "fugaescorregadia",
  "version": "0.6.1",
  "description": "Jogo de labirinto / Labyrinth game",
  "main": "index.js",
  "scripts": {
    "start": "npx parcel -p 1234 src/index.html",
    "editor": "npx parcel -p 2345 src/level-editor.html",
    "tdd": "npx parcel -p 3456 src/test-browser-unit-integration.html --no-cache",
    "autoplay": "npx parcel -p 4567 src/test-browser-system.html --no-cache",
    "test": "npx parcel build src/test-node-unit-integration.coffee --no-cache --no-minify && npx mocha dist/test-node-unit-integration*.js",
    "parcel-build": "npx parcel build src/index.html --public-url=.",
    "deploy": "cp scripts/deploy.sh /tmp/deploy.sh && bash /tmp/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awvalenti/fugaescorregadia.git"
  },
  "keywords": [
    "jogo",
    "labirinto",
    "game",
    "labyrinth"
  ],
  "author": "Andre Willik Valenti <awvalenti@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/awvalenti/fugaescorregadia/issues"
  },
  "homepage": "https://github.com/awvalenti/fugaescorregadia#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "coffeescript": "^2.4.1",
    "mocha": "^6.1.3",
    "parcel-bundler": "^1.12.3",
    "posthtml-modules": "^0.4.2",
    "semver": "^6.0.0",
    "should": "^13.2.3"
  }
}
